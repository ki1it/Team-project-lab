head


    meta(charset='utf-8')
    meta(http-equiv='x-ua-compatible' content='ie=edge')
    title СТО
    meta(name='description' content='A high-quality & free Bootstrap admin dashboard template pack that comes with lots of templates and components.')
    meta(name='viewport' content='width=device-width, initial-scale=1, shrink-to-fit=no')
    link(href='https://use.fontawesome.com/releases/v5.0.6/css/all.css' rel='stylesheet')
    link(href='https://fonts.googleapis.com/icon?family=Material+Icons' rel='stylesheet')
    link(rel='stylesheet' href='https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css' integrity='sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO' crossorigin='anonymous')
    link#main-stylesheet(rel='stylesheet' data-version='1.1.0' href='styles/shards-dashboards.1.1.0.min.css')
    link(href='/dist/css/datepicker.min.css', rel='stylesheet')


    script(src='https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js')
    script(src='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js')

    #myModal.modal.fade(role='dialog')
        .modal-dialog.modal-lg
            .modal-content
                .modal-header
                    h4.modal-title Добавление юридического клиента
                    button.close(type='button', data-dismiss='modal') ×

                .modal-body
                    ul.list-group.list-group-flush
                        form(action="/addUrClient", method="post" class="form" id='addUrClient' enctype="application/json")
                            li.list-group-item.p-3
                                .row
                                    .col-sm-12
                                        strong.text-muted.d-block.mb-2 Название
                                            .form-group
                                                input.form-control(type='text' name="inputName" required=true)
                                    .col-sm-12
                                        strong.text-muted.d-block.mb-2 ИНН
                                            .form-group
                                                input.form-control(type='text' name="inputINN" required=true)
                                    .col-sm-12
                                        strong.text-muted.d-block.mb-2 Номер телефона
                                            .form-group
                                                input.form-control(type='text' name="inputPhoneNumber" required=true maxlength='11' minlength='11')

                                    .col-sm-12
                                        strong.text-muted.d-block.mb-2 Адресс
                                            .form-group
                                                input.form-control(type='text' name="inputAdress")
                                    .col-lg-12.mb-4
                                        .row
                                            .col
                                                a.nav-link(href='client')
                                                    button.btn.btn-primary.btn-lg.btn-block(type="submit") Добавить
    #editEmployeeModal.modal.fade
        .modal-dialog
            .modal-content
                form
                    .modal-header
                        h4.modal-title Edit Employee
                        button.close(type='button' data-dismiss='modal' aria-hidden='true') &times;
                    .modal-body
                        .form-group
                            label Name
                            input.form-control(type='text' required='')
                        .form-group
                            label Email
                            input.form-control(type='email' required='')
                        .form-group
                            label Address
                            textarea.form-control(required='')
                        .form-group
                            label Phone
                            input.form-control(type='text' required='')
                    .modal-footer
                        input.btn.btn-default(type='button' data-dismiss='modal' value='Cancel')
                        input.btn.btn-info(type='submit' value='Save')



    link(rel='stylesheet' href='styles/extras.1.1.0.min.css')
    script(async='' defer='' src='https://buttons.github.io/buttons.js')
    script(async='' defer='' src='https://www.kryogenix.org/code/browser/sorttable/sorttable.js')
    - var i=0;
        mixin rowsTable(id, Name, INN, Tel, Adress)
            tr

                //td #{id}
                td #{Name}
                td #{INN}
                td #{Tel}
                td #{Adress}
                td
                    a#navbarDropdownMenuLink.nav-link(href='#' data-toggle='dropdown' aria-haspopup='true' aria-expanded='false')
                        i.material-icons menu
                        p.d-lg-none.d-md-block
                            | Some Actions
                    .dropdown-menu.dropdown-menu-right(aria-labelledby='navbarDropdownMenuLink')
                        a.dropdown-item(href='/urclient' onclick="$.ajax({type: 'POST', url: '/delurclient', data: 'INN=" + INN + "', })") Удалить
                        a.dropdown-item(href='/editUrClient?INN='+INN ) Изменить


.container-fluid
    .row
        // Main Sidebar
        aside.main-sidebar.col-12.col-md-3.col-lg-2.px-0
            .main-navbar
                nav.navbar.align-items-stretch.navbar-light.bg-white.flex-md-nowrap.border-bottom.p-0
                    a.navbar-brand.w-100.mr-0(href='#' style='line-height: 25px;')
                        .d-table.m-auto
                            img#main-logo.d-inline-block.align-top.mr-1(style='max-width: 25px;' src='images/shards-dashboards-logo.svg' alt='Shards Dashboard')
                            span.d-none.d-md-inline.ml-1 АРМ Администратор
                    a.toggle-sidebar.d-sm-inline.d-md-none.d-lg-none
                        i.material-icons &#xE5C4;
            .nav-wrapper
                ul.nav.flex-column
                    li.nav-item
                        a.nav-link(href='index')
                            i.material-icons edit
                            span Главная
                    li.nav-item
                        a.nav-link.dropdown-toggle(href='servicelist'  data-toggle='dropdown' aria-haspopup='true' aria-expanded='false')
                            i.material-icons vertical_split
                                span Заказ-наряд
                            .dropdown-menu(aria-labelledby='dropdownMenuLink')
                                a.dropdown-item(href='servicelist') Поиск
                                a.dropdown-item(href='addservicelist?ID=') Добавить
                                a.dropdown-item(href='#') Что-то еще


                    li.nav-item
                        a.nav-link.active.dropdown-toggle(href='servicelist'  data-toggle='dropdown' aria-haspopup='true' aria-expanded='false')
                            i.material-icons note_add
                                span Справочники
                            .dropdown-menu(aria-labelledby='dropdownMenuLink')
                                a.dropdown-item(href='client') Физические клиенты
                                a.active.dropdown-item(href='urclient') Юридические клиенты
                                a.dropdown-item(href='car') Автомобили
                                a.dropdown-item(href='breakdown') Поломки
                                a.dropdown-item(href='service') Услуги
                                a.dropdown-item(href='detail') Запчасти
                    li.nav-item
                        a.nav-link(href='forms')
                            i.material-icons view_module
                            span Формы
                    li.nav-item
                        a.nav-link(href='about')
                            i.material-icons table_chart
                            span О сайте
        // End Main Sidebar
        main.main-content.col-lg-10.col-md-9.col-sm-12.p-0.offset-lg-2.offset-md-3
            .main-navbar.sticky-top.bg-white
                // Main Navbar
                nav.navbar.align-items-stretch.navbar-light.flex-md-nowrap.p-0
                    form.main-navbar__search.w-100.d-none.d-md-flex.d-lg-flex(action='#')

                    ul.navbar-nav.border-left.flex-row
                        li.nav-item.border-right.dropdown.notifications
                            a#dropdownMenuLink.nav-link.nav-link-icon.text-center(href='#' role='button' data-toggle='dropdown' aria-haspopup='true' aria-expanded='false')
                                .nav-link-icon__wrapper
                                    i.material-icons &#xE7F4;
                                    span.badge.badge-pill.badge-danger 2
                            .dropdown-menu.dropdown-menu-small(aria-labelledby='dropdownMenuLink')
                                a.dropdown-item(href='#')
                                    .notification__icon-wrapper
                                        .notification__icon
                                            i.material-icons &#xE6E1;
                                    .notification__content
                                        span.notification__category Analytics
                                        p
                                            | Your website&rsquo;s active users count increased by
                                            span.text-success.text-semibold 28%
                                            |  in the last week. Great job!
                                a.dropdown-item(href='#')
                                    .notification__icon-wrapper
                                        .notification__icon
                                            i.material-icons &#xE8D1;
                                    .notification__content
                                        span.notification__category Sales
                                        p
                                            | Last week your store&rsquo;s sales count decreased by
                                            span.text-danger.text-semibold 5.52%
                                            | . It could have been worse!
                                a.dropdown-item.notification__all.text-center(href='#')  View all Notifications
                        li.nav-item.dropdown
                            a.nav-link.dropdown-toggle.text-nowrap.px-3(data-toggle='dropdown' href='#' role='button' aria-haspopup='true' aria-expanded='false')
                                img.user-avatar.rounded-circle.mr-2(src='images/avatars/0.jpg' alt='User Avatar')
                                span.d-none.d-md-inline-block Даша Марьина
                            .dropdown-menu.dropdown-menu-small
                                .dropdown-divider
                                a.dropdown-item.text-danger(href='#')
                                    i.material-icons.text-danger &#xE879;
                                    |  Logout
                    nav.nav
                        a.nav-link.nav-link-icon.toggle-sidebar.d-md-inline.d-lg-none.text-center.border-left(href='#' data-toggle='collapse' data-target='.header-navbar' aria-expanded='false' aria-controls='header-navbar')
                            i.material-icons &#xE5D2;
            // / .main-navbar
            .main-content-container.container-fluid.px-4
                // Page Header
                .page-header.row.no-gutters.py-4
                    .col-12.col-sm-4.text-center.text-sm-left.mb-0
                        //span.text-uppercase.page-subtitle Overview
                        h3.page-title Юридические клиенты
                // End Page Header
                // Default Light Table
                .row
                    .col
                        .card.card-small.mb-4
                            .card-header.border-bottom
                                //h6.m-0 Заказ-наряд
                                .row
                                    //    //.col-sm-12.col-md-12
                                    //        h6.m-0 Заказ-наряд
                                    //    .col-sm-12.col-md-12
                                    //        .input-group.input-group-seamless
                                    //            .input-group-prepend
                                    //                .input-group-text
                                    //                    i.fas.fa-search
                                    //            input.navbar-search.form-control(type='text' placeholder='поиск' aria-label='Search')
                                    form.form-inline.col-sm-12.col-md-12(action="/urclient")
                                        .col-sm-12.col-md-11
                                            input.form-control(type='text' name='INN')
                                            button.btn.btn-primary(href='' type='submit') Найти
                                        .col-sm-12.col-md-1
                                            button.btn.btn-primary(type='button', data-toggle='modal', data-target='#myModal') +


                            .card-body.p-0.pb-3.text-center
                                table.table.mb-0.sortable
                                    thead.bg-light.text-primary
                                        tr
                                            //th.border-0(scope='col') №
                                            th.border-0(scope='col') Название
                                            th.border-0(scope='col') ИНН
                                            th.border-0(scope='col') Телефон
                                            th.border-0(scope='col') Адрес
                                            th.border-0(scope='col') Действие
                                    tbody
                                        -for (let i = 0; i < client.length; i++)
                                            +rowsTable(client[i].dataValues.id,
                                            client[i].dataValues.Name,
                                            client[i].dataValues.INN,
                                            client[i].dataValues.PhoneNumber,
                                            client[i].dataValues.Adress)
            footer.main-footer.d-flex.p-2.px-3.bg-white.border-top
                span.copyright.ml-auto.my-auto.mr-2
                    Copyright &copy; 2019 ki1it&ma-daria

script(src='https://code.jquery.com/jquery-3.3.1.min.js' integrity='sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8=' crossorigin='anonymous')
